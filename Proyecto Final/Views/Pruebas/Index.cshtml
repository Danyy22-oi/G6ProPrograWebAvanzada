@model IEnumerable<Proyecto.Models.Pruebas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PresionBalloon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InspeccionTactil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RupturaBalloon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Desinflado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cateter)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PresionBalloon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InspeccionTactil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RupturaBalloon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Desinflado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cateter.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
